#include<iostream>
using namespace std;

int main(){
    
    int n,i,j;
    n = 4;
    for(int i = 1 ; i <= n ; i++ ){
    for(int j = 1 ; j <= i; j++){
        cout<<j<<" ";
    }    
    
    cout<<"\n";
    }
    
    return 0;
}


// Output:
// 1       -> First line prints 1
// 12      -> Second line prints 1 and 2
// 123     -> Third line prints 1, 2, and 3
// 1234    -> Fourth line prints 1, 2, 3, and 4
